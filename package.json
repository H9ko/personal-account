{
  "name": "personal-account",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "npx webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack serve --mode development --devtool inline-source-map --hot",
    "server": "npx nodemon src/server/index.js",
    "dev": "npx kill-port 3000 8080 && npx concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "npm run build"
  },
  "author": "Vova Afanasiev",
  "license": "ISC",
