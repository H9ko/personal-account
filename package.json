    "bootstrap-icons": "^1.1.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "formik": "^2.2.5",
    "json-server": "^0.16.2",
    "json-server-auth": "^2.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "yup": "^0.29.3",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
