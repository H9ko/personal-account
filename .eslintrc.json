{
  "plugins": [
    "react",
    "jsx-a11y",
    "react-hooks",
    "prettier"
  ],
  "parser": "babel-eslint",
  "extends": ["airbnb",
    "plugin:prettier/recommended",
    "prettier/flowtype",
    "prettier/react",
    "prettier/standard"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/extensions": 0,
    "react/prop-types": 0,
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "prettier/prettier": ["error", {"singleQuote": true, "parser": "flow"}]
  }
}